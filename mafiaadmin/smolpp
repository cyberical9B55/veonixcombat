	else
		FLYSPEED = 20
		loadstring(game:HttpGet("https://gist.githubusercontent.com/cyberical9B55/b7c482e5595261af3d667f3f673c2620/raw/4b8c78a75f71e19bc2d0547234390fa0b802ebfc/flight"))()
